.container {
  min-width: 920px;
}

header {
  min-width: 940px;
  background-color: #000;
  overflow: hidden;
  position: fixed;
  top:0;
  width: 100%;
  z-index: 100;
}

div.logo {
  float: left;
  padding: 6px;
  width: 194px;
  height: 34px;
  margin-right: 50px;
  margin-left: 30px;
}

nav ul {
  margin: 0;
}

nav ul li {
  float: left;
}

a.first-button, a.second-button {
  background-color: #2f3636;
  padding: 15px;
  font-size: 12px;
  display: block;
}

nav ul li a {

}

a.first-button {
  background-color: #ffdc00;
  color: #000;
}

nav ul li a {
  color: #fff;
  text-decoration: none;
  text-transform: uppercase;
  font-weight: 900;
}

section {
  min-width: 940px;
  width: 100%;
  height: 100vh;
  min-height: 500px;
  overflow: hidden;
}

div#map {
  margin-top: 46px;
  width: 80%;
  height: 100vh;
  position: relative;
  box-sizing: border-box;
  float: left;
  padding-top: 45px;
}

div.listing {
  width: 20%;
  height: 100vh;
  box-sizing: border-box;
  padding-top: 45px;
  overflow: hidden;
  float: left;
  background-color: #2f3636;
}

div.listing-inner {
  height: 100%;
  overflow: scroll;
}

div.listing ul {
  margin: 0;
  padding: 0;
}

div.listing ul li {
  list-style: none;
  margin: 0;
  padding: 0;
}

div.listing-inner > ul > li > span {
  display: block;
  font-size: 14px;
  color: #fff;
  padding: 10px;
  cursor: pointer;
}

div.listing .content {
  display: none;
}

div.listing .content ul li span {
  display: block;
  font-size: 12px;
  color: #fff;
  padding: 5px;
  padding-left: 25px;
  cursor: pointer;
  background-color: #000;
  border-bottom: 1px solid #333;
}

div.listing ul li span i.fa {
  margin-right: 10px;
  width: 20px;
  text-align: center;
}

#startupsButton {
  border-left: 6px solid #e92b2b;
}

div.listing #startupsButton + .content {
  border-left: 6px solid #e92b2b;
}

#investorsButton {
  border-left: 6px solid #78d513;
}

div.listing #investorsButton + .content {
  border-left: 6px solid #78d513;
}

#acceleratorsButton {
  border-left: 6px solid #3af0ee;
}

div.listing #acceleratorsButton + .content {
  border-left: 6px solid #3af0ee;
}

#incubatorsButton {
  border-left: 6px solid #c33af0;
}

div.listing #incubatorsButton + .content {
  border-left: 6px solid #c33af0;
}

#coworkingButton {
  border-left: 6px solid #ffab26;
}

div.listing #coworkingButton + .content {
  border-left: 6px solid #ffab26;
}

#eventsButton {
  border-left: 6px solid #3af0ee;
}

.center {
  text-align: center;
}

.about-image {
  padding: 30px;
}

.about-footer {
  padding-top: 30px;
}

.about-footer p {
  font-size: 14px;
  color: #999;
}

.submission-container p {
  font-size: 12px;
}

.infowindow {
  max-width: 200px;
  overflow: hidden;
}

.infowindow p {
  margin: 0;
  font-size: 11px;
  margin-bottom: 5px;
}

.infowindow p b {
  font-size: 12px;
  margin-left: 10px;
}

.infowindow p a {
  color: #0DB6B4;
  text-decoration: none;
}


.half {
  width: 50%;
  overflow: hidden;
  float: left;
  box-sizing: border-box;
  padding: 10px;
}

.form-group {
  padding: 10px 0;
}

label {
  display: block;
  float: left;
  width: 90px;
  font-size: 12px;
  padding-bottom: 4px;
}

input, textarea {
  width: 90%;
  border: 1px solid #ddd;
  background-color: #fff;
  padding: 10px;
  font-size: 14px;
}

input:focus, textarea:focus {
  outline: 0;
  box-shadow: none;
  border: 1px solid #000;
}

.cta-box {
  border-top: 1px solid #ddd;
  padding-top: 20px;
}

.md-content button {
  background-color: #000;
  color: #fff;
  padding: 15px 40px;
  border: 0;
  font-size: 14px;
  font-weight: bold;
  text-transform: uppercase;
  position: relative;
}

.md-content button:before {
  content: '\f0a9';
  font-family: 'FontAwesome';
  position: absolute;
  left: -100px;
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
}

.md-content button:hover:before {
  left: 15px;
}

#modal-1 {
  width: 80%;
  max-width: 790px;
}

.form-description {
  font-size: 12px;
  color: #333;
  border-bottom: 1px solid #ddd;
  line-height: 18px;
}

p.address {
  /*min-height: 30px;*/
  color: #999;
  font-style: italic;
}

#logoClick {
  cursor: pointer;
}

#searchField {
  height: 100%;
  padding: 14px;
  color: #fff;
  font-size: 12px;
  background-color: #000;
  border: 0;
  padding-left: 47px;
  background-image: url(<%= asset_path 'search-icon.png' %>);
  background-repeat: no-repeat;
  width: 300px;
  box-sizing: border-box;
}

.search-container {
  position: relative;
  float: left;
  width: 300px;
}

#searchField:focus {
  background-image: url(<%= asset_path 'search-icon-selected.png' %>);
}


ul.ui-autocomplete {
  padding: 0;
  margin: 0;
  width: 284px !important;
}

.ui-state-focus {
  background-color: #2F3636 !important;
}

.search-clear {
  position: absolute;
  right: -10px;
  top: 10px;
  color: #666;
  cursor: pointer;
}

.success-box {
  display: none;
  color: #78d513;
  width: 80%;
  margin: auto;
  text-align: center;
  padding: 50px;
}

.success-box p {
  font-size: 18px;
}